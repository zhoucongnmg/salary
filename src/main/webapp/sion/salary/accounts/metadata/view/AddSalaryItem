                    "",
                    "if(type === '系统提取项'){",
                    "    system = true;",
                    "}",
                    "store.clearFilter(true);",
                    "Ext.apply(store.proxy.extraParams, {",
                    "    system : system,",
                    "    type : type",
                    "});",
                    "store.load({",
                    "    scope: this,",
                    "    callback: function(records, operation, success) {",
                    "        var record = store.find('id', accountItem.get('salaryItemId'));",
                    "        grid.getSelectionModel().select(record);",
                    "    }",
                    "});"
