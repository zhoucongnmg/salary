                    "    taxId.setValue(record.get('taxId'));",
                    "}",
                    "",
                    "if (me._data) {",
                    "    taxStore.removeAll();",
                    "    Ext.Array.each(data,function(d,index){",
                    "        var record = Ext.create(ns + '.model.Item',{",
                    "            id: d.get('id'),",
                    "            name : d.get('name')",
                    "        });",
                    "        taxStore.add(record);",
