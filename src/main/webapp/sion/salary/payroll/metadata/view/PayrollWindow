            "userConfig": {
                "designer|params": [
                    "node"
                ],
                "fn": "checkParent",
                "implHandler": [
                    "node = node.parentNode;",
                    "if(!node) return;",
                    "var checkP=false;",
                    "node.cascadeBy(function (n)",
                    "               {",
                    "                   if (n != node) {",
                    "                       if (n.get('checked') == true) {",
                    "                           checkP = true;",
                    "                       }",
                    "                   }",
                    "               });",
                    "node.set('checked', checkP);",
                    "this.checkParent(node);",
                    ""
                ]
            },
            "name": "checkParent",
            "designerId": "4a818bbb-c0e0-4728-8404-ea9a01b68841"
