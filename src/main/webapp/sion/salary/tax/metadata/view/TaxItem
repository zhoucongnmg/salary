            "codeClass": null,
            "userConfig": {
                "designer|params": [
                    "prevEnd",
                    "prevRate",
                    "prevFastNumber",
                    "rate"
                ],
                "fn": "countFastNumber",
                "implHandler": [
                    "var value = 0;",
                    "",
                    "value = prevEnd * ((rate * 0.01).toFixed(2) - prevRate);",
                    "value = parseFloat(value) + parseFloat(prevFastNumber);",
                    "return value;"
                ]
            },
            "name": "countFastNumber",
            "designerId": "56a03111-94ef-4f8a-8196-cb359e4bdd2a"
